{
    "tags": {
      "allowUnknownTags": true
    },
    "opts": {
      "destination": "./doc/doc-pages"
    },
    "plugins": [
      "plugins/markdown"
    ],
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false,
      "default": {
        "outputSourceFiles": true,
        "useLongnameInNav": true
      },
      "logo": {
        "url": "https://openlayers.org/en/latest/examples/resources/logo-70x70.png",
        "width": "30px",
        "height": "30px",
        "link": "https://github.com/Viglino/ol3-ext"
      },
      "systemName": "ol3-ext",
      "path": "ink-docstrap",
      "theme": "cerulean",
      "navType": "vertical",
      "linenums": true,
      "dateFormat": "MMMM Do YYYY, h:mm:ss a"
    }
  }